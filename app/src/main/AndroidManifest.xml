<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobile.ucsal.br.eventosapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_eventos"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_eventos"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BuscarActivity" />
        <activity android:name=".TipoActivity" />
        <activity android:name=".EventoActivity"></activity>
    </application>

</manifest>